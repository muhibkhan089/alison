<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Manage Wishlists</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Cminds\Multiwishlist\Block\Wishlist\Manage\Table" name="wishlists.manage.table"
                   template="Cminds_Multiwishlist::wishlist/manage/table.phtml"
                   cacheable="false"/>
        </referenceContainer>
    </body>
</page>